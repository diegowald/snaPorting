<Window x:Class="Catalogo._preferencias.passwordFrm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Ingrese la clave" Height="150" Width="300">
    <Grid VerticalAlignment="Center">
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="50*"/>
            <ColumnDefinition Width="50*"/>
        </Grid.ColumnDefinitions>
        <Label Grid.Row="0" Grid.Column="0" Content="Clave"/>
        <PasswordBox Grid.Column="1" Grid.Row="0" Height="23" Name="txtPwd"/>
        <Button Content="OK" Grid.Column="0" Grid.Row="1" Name="OKButton" Click="OKButton_Click_1"/>
        <Button Content="Cancel" Grid.Column="1" Grid.Row="1" Name="CancelButton" Click="CancelButton_Click_1"/>
    </Grid>
</Window>

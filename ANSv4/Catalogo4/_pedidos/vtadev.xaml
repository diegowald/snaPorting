<Window x:Class="Catalogo._pedidos.vtadev"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ad="clr-namespace:AvalonDock;assembly=AvalonDock"        
        Title="Venta y Devoluciones" Height="300" Width="300">
    <Grid>
        <ad:DockablePane x:Name="misTabsAcciones" SelectedIndex="0" Visibility="Visible">
            <ad:DockableContent Title="Nota de Venta" x:Name="notaDeVenta">
                <ListView>
                    <ListView.View>
                        <GridView>
                            <GridView.Columns>
                                <GridViewColumn Header="Comprar"/>
                                <GridViewColumn Header="Ver"/>
                                <GridViewColumn Header="Confirmar"/>
                                <GridViewColumn Header="Nuevo"/>
                            </GridView.Columns>
                        </GridView>
                    </ListView.View>
                </ListView>
            </ad:DockableContent>
            <ad:DockableContent Title="Devoluciones" x:Name="devoluciones"></ad:DockableContent>
        </ad:DockablePane>


    </Grid>
</Window>
